{
    "baseUrl": "..",
    "include": [
        "lib/bootstrap",
        "lib/jquery.deferredImage",
        "lib/jquery.ui.sliderX",
        "lib/jquery.isotope"
    ],
    "name": "app/TvFinderApp",
    "out": "../target/index.js",
    "paths": {
        "requirejs": "/usr/local/lib/node_modules/requirejs/require",
        "handlebars": "/usr/local/lib/node_modules/handlebars/dist/handlebars.runtime",
        "Backbone": "/usr/local/lib/node_modules/backbone/backbone",
        "jQuery": "lib/jquery-1.7.2",
        "underscore": "/usr/local/lib/node_modules/underscore/underscore",
        "content": "lib/content-client",
        "televisions": "lib/televisions-client",
        "environment": "lib/environment"
    },
    "shim": {
        "Backbone": {
            "deps": [
                "underscore",
                "jQuery"
            ],
            "exports": "Backbone"
        },
        "jQuery": {
            "exports": "$"
        },
        "underscore": {
            "exports": "_"
        },
        "handlebars": {
            "exports": "Handlebars"
        },
        "lib/jquery.deferredImage" : {
            "deps": [
                "jQuery",
                "lib/jquery.viewed"
            ]
        },
        "lib/jquery.viewed" : {
            "deps": [
                "jQuery"
            ]
        },
        "lib/jquery.isotope" : {
            "deps": [
                "jQuery"
            ]
        },
        "lib/jquery.ui.sliderX": {
            "deps": [
                "lib/jquery.ui.slider"
            ]
        },
        "lib/jquery.ui.slider": {
            "deps": [
                "lib/jquery.ui.widget",
                "lib/jquery.ui.mouse"
            ]
        },
        "lib/jquery.ui.mouse": {
            "deps": [
                "lib/jquery.ui.widget"
            ]
        },
        "lib/jquery.ui.widget": {
            "deps": [
                "lib/jquery.ui.core"
            ]
        },
        "lib/bootstrap" : {
            "deps": [
                "jQuery"
            ]

        }
    }
}